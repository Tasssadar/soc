t <span class=3D"operator">=3D</span> <span class=3D"type"><a href=3D"qr=
ectf.html">QRectF</a></span>(<span class=3D"number">10</span><span class=
=3D"operator">,</span> <span class=3D"number">20</span><span class=3D"op=
erator">,</span> <span class=3D"number">30</span><span class=3D"operator=
">,</span> <span class=3D"number">40</span>);
